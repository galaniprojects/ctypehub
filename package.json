{
  "repository": "git@github.com:galaniprojects/ctypehub.git",
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": ">= v18.16.1"
  },
  "scripts": {
    "prettify": "prettier --write \"src/**/*.astro\" \"src/**/*.ts*\" \"src/**/*.css*\"",
    "lint": "yarn run lint-css --formatter github && yarn run lint-js",
    "lint-js": "eslint --max-warnings 0 --config ./.eslintrc.json \"src/**/*.astro\" \"src/**/*.ts*\"",
    "lint-css": "stylelint --max-warnings 0 --fix \"src/**/*.astro\" \"src/**/*.css\"",
    "test": "tsc --noEmit && astro check && vitest",
    "dev": "astro dev",
    "start-db": "docker run -d --rm --name postgres -e POSTGRES_PASSWORD=postgres -p 5432:5432 postgres",
    "stop-db": "docker rm -f postgres",
    "migrate": "sequelize-cli db:migrate",
    "build": "export $(cat .env.example) && astro build",
    "start": "yarn migrate && node --enable-source-maps dist/server/entry.mjs"
  },
  "dependencies": {
    "@astrojs/node": "^5.3.0",
    "@astrojs/react": "^2.2.1",
    "@kiltprotocol/sdk-js": "^0.33.1",
    "@polkadot/extension-dapp": "^0.46.5",
    "astro": "^2.8.3",
    "got": "^12.6.1",
    "http-status-codes": "^2.2.0",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "pino": "^8.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "slash": "^4.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.16.19",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "buffer": "^6.0.3",
    "cbor-web": "^9.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-astro": "^0.27.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vitest": "^0.2.6",
    "jest-serializer-vue": "^3.1.0",
    "jsdom": "^22.1.0",
    "pino-pretty": "^10.0.1",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.11.0",
    "stylelint": "^15.10.1",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^34.0.0",
    "testcontainers": "^9.11.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0",
    "vitest-github-actions-reporter": "^0.10.0"
  }
}
